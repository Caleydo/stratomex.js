{
  "name" : "stratomex.js",
  "version": "0.0.1",
  "dependencies": {
    "d3": "~3.5.2",
    "bootstrap" : "~3.3.1"
  },
  "caleydo" : {
    "plugins": [
      {
        "type" : "actionFactory",
        "id" : "stratomex-note",
        "folder" : "stratomex.js",
        "module" : "stratomex.js/notes",
        "factory" : "createCmd",
        "creates" : "(addNote|changeNote|removeNote)"
      },
      {
        "type" : "actionFactory",
        "id" : "stratomex-column",
        "folder" : "stratomex.js",
        "module" : "stratomex.js/column",
        "factory" : "createCmd",
        "creates" : "(createColumn|removeColumn|setColumnOption|changeColumnVis)"
      }
    ],
    "requirejs-config": {
      "paths": {
        "bootstrap": "${baseUrl}/bootstrap/dist/js/bootstrap"
      },
      "shim": {
        "bootstrap": ["css!${baseUrl}/bootstrap/dist/css/bootstrap"]
      }
    }
  }
}
