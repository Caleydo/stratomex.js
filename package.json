{
  "name" : "caleydo-vis-genome",
  "version": "0.0.1",
  "dependencies": {
    "d3": "~3.5.2",
    "bootstrap" : "~3.3.1"
  },
  "caleydo" : {
    "plugins": [
      {
        "type": "vis",
        "id": "caleydo-vis-genome",
        "name": "Genome",
        "filter": "genomeDataLink"
      },
      {
        "type": "datatype",
        "id": "genomeDataLink",
        "folder" : "caleydo-vis-genome",
        "module": "caleydo-vis-genome/genomeDataLink"
      }
    ],
    "requirejs-config": {
      "paths": {
        "bootstrap": "${baseUrl}/bootstrap/dist/js/bootstrap"
      },
      "shim": {
        "bootstrap": ["css!${baseUrl}/bootstrap/dist/css/bootstrap"]
      }
    }
  }
}
