{
  "name": "stratomex.js",
  "version": "0.0.1",
  "peerDependencies": {
    "caleydo_web": "*",
    "caleydo_selectioninfo": "*",
    "caleydo_links": "*",
    "caleydo_provenance": "*",
    "caledyo_vis": "*"
  },
  "caleydo": {
    "plugins": {
      "web": [
        {
          "type": "view",
          "id": "stratomex",
          "file": "stratomex",
          "type": "main",
          "location": "center"
        },
        {
          "type": "view",
          "id": "stratomex-lineup",
          "file": "lineup",
          "type": "support",
          "location": "bottom"
        },
        {
          "type": "actionFactory",
          "id": "stratomex-note",
          "file": "notes",
          "factory": "createCmd",
          "creates": "(addNote|changeNote|removeNote)"
        },
        {
          "type": "actionFactory",
          "id": "stratomex-column",
          "file": "column",
          "factory": "createCmd",
          "creates": "(createColumn|removeColumn|setColumnOption|changeColumnVis|showInDetail)"
        },
        {
          "type": "app",
          "name": "Stratomex.js"
        }
      ]
    },
    "dependencies": {
      "web": {
        "d3": "~3.5.5",
        "bootstrap": "~3.3.4",
        "marked": "~0.3.3"
      }
    },
    "requirejs-config": {
      "paths": {
        "bootstrap": "${baseUrl}/bootstrap/dist/js/bootstrap"
      },
      "shim": {
        "bootstrap": [
          "css!${baseUrl}/bootstrap/dist/css/bootstrap"
        ],
        "webcola": {
          "deps": [
            "d3"
          ],
          "exports": "cola"
        }
      }
    }
  }
}
